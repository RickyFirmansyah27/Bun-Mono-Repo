{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/auth/(.*)", "dest": "/auth/$1" },
    { "src": "/express/(.*)", "dest": "/express/$1" },
    { "src": "/hono/(.*)", "dest": "/hono/$1" },
    { "src": "/elysia/(.*)", "dest": "/elysia/$1" },
    { "src": "/fastify/(.*)", "dest": "/fastify/$1" },
    { "src": "/koa/(.*)", "dest": "/koa/$1" }
  ]
}
