{
  "name": "fastify-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "clean": "rm -rf dist",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint . --ext .ts",
    "check-types": "tsc --noEmit",
    "start": "bun run dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fastify": "^5.1.0",
    "moment": "^2.30.1",
    "winston": "^3.17.0"
  }
}